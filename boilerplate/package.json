{
  "name": "app",
  "productName": "App",
  "version": "0.0.0",
  "description": "",
  "license": "MIT",
  "main": "index.js",
  "repository": "user/repo",
  "author": {
    "name": "",
    "email": "",
    "url": ""
  },
  "engines": {
    "node": ">=0.10.0"
  },
  "electronVersion": "0.26.0",
  "scripts": {
    "start": "electron .",
    "build-osx": "trash dist --force && electron-packager . $npm_package_productName --prune --asar --platform=darwin --arch=x64 --version=$npm_package_electronVersion --out=dist",
    "build-linux": "trash dist --force && electron-packager . $npm_package_productName --prune --asar --platform=linux --arch=x64 --version=$npm_package_electronVersion --out=dist",
    "build-win": "trash dist --force && electron-packager . $npm_package_productName --prune --asar --platform=win32 --arch=x64 --version=$npm_package_electronVersion --out=dist",
    "build": "npm run build-osx & npm run build-linux & npm run build-win"
  },
  "files": [
    "index.js",
    "index.html",
    "index.css"
  ],
  "keywords": [
    "electron-app"
  ],
  "devDependencies": {
    "electron-packager": "^4.0.3",
    "electron-prebuilt": "^0.26.0",
    "trash": "^1.4.1"
  }
}
